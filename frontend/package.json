{
  "name": "bbb_content_generator_frontend",
  "version": "1.5.1",
  "private": true,
  "engines": {
    "node": "20.19.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/index.{ts,tsx,js,jsx}",
      "!src/**/*.stories.{ts,tsx,js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 75,
        "branches": 70,
        "functions": 75,
        "lines": 75
      }
    }
  },
  "scripts": {
    "start": "env-cmd -f .env react-scripts start",
    "serve": "serve -s build",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prisma:generate": "mkdir -p prisma && cp ../backend/prisma/schema.prisma prisma/ && npx prisma generate",
    "generate:types:local": "yarn run generate:api-types:local && yarn run generate:prisma-schema:local",
    "generate:api-types:local": "npx openapi-typescript http://localhost:8080/api-docs/json -o src/types/api.d.ts",
    "generate:prisma-schema:local": "mkdir -p prisma && curl http://localhost:8080/prisma-schema -o prisma/schema.prisma",
    "generate:types:dev": "npx openapi-typescript https://backend-app-754352681293.us-central1.run.app/api-docs/json -o src/types/api.d.ts",
    "build:ts": "tsc",
    "lint": "eslint src --max-warnings=0",
    "lint:fix": "eslint src --fix",
    "lint:ci": "yarn lint",
    "format": "prettier --cache --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test:coverage": "react-scripts test --watchAll=false --coverage --runInBand",
    "coverage": "react-scripts test --watchAll=false --coverage --runInBand",
    "test:ci": "yarn test:coverage"
  },
